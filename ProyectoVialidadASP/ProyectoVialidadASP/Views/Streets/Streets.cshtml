
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Streets</title>
    <link href="~/Style/StyleIndex/SStreets.css" rel="stylesheet" />
</head>
<body>
    <section>
        <form action="Streets" method="post">
            <div class="container">
                <h2>Agregar Calle No Disponibles</h2>
                <label style="color:red" name="ERROR" id="error">@ViewBag.Message1</label>
                <div class="row100">
                    <div class="col">
                        <div class="inputBox">
                            <input type="text" name="NLugar" id="" required>
                            <span class="text">Nombre del lugar</span>
                            <span class="line"></span>
                        </div>
                    </div>

                </div>
                <div class="row100">
                    <div class="col">
                        <div class="inputBox">
                            <input type="text" name="NCalle" id="" required>
                            <span class="text">Nombres de las calles</span>
                            <span class="line"></span>
                        </div>
                    </div>
                    <div class="col">
                        <div class="inputBox">
                            <input type="text" name="Descripcion" id="" required>
                            <span class="text">Descripcion</span>
                            <span class="line"></span>
                        </div>
                    </div>
                </div>
                <div class="row100">
                    <div class="col">
                        <div class="inputBox">

                            <span class="text">Fecha de Programacion</span>
                        </div>
                    </div>
                    <div class="col">
                        <div class="inputBox">
                            <input type="date" name="FProgramacion" id="" required>

                            <span class="line"></span>
                        </div>
                    </div>
                </div>
                <div class="row100">
                    <div class="col">
                        <div class="inputBox">
                            <input type="time" name="TiempoI" id="" required>
                            <span class="text">Tiempo Inicio</span>
                            <span class="line"></span>
                        </div>
                    </div>
                    <div class="col">
                        <div class="inputBox">
                            <input type="time" name="TiempoF" id="" required>
                            <span class="text">Tiempo Final</span>
                            <span class="line"></span>
                        </div>
                    </div>
                </div>
                <h3>Punto de Inicio</h3>
                <div class="row100">
                    <div class="col">
                        <div class="inputBox">
                            <input type="text" name="Latitud1" id="" required>
                            <span class="text">Latitud</span>
                            <span class="line"></span>
                        </div>
                    </div>
                    <div class="col">
                        <div class="inputBox">
                            <input type="text" name="Longitud1" id="" required>
                            <span class="text">Longitud</span>
                            <span class="line"></span>
                        </div>
                    </div>
                </div>
                <h3>Punto de Cierre</h3>
                <div class="row100">
                    <div class="col">
                        <div class="inputBox">
                            <input type="text" name="Latitud2" id="" required>
                            <span class="text">Latitud</span>
                            <span class="line"></span>
                        </div>
                    </div>
                    <div class="col">
                        <div class="inputBox">
                            <input type="text" name="Longitud2" id="" required>
                            <span class="text">Longitud</span>
                            <span class="line"></span>
                        </div>
                    </div>
                </div>
                <div class="row100">
                    
                    <div class="col">
                        <input type="submit" value="Programar">
                    </div>
                    
                </div>

            </div>
        </form>
    </section>
</body>
</html>
